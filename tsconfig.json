{
  "compilerOptions": {
    "baseUrl": "./src",
    "allowSyntheticDefaultImports": true,
    "jsx": "react",
    "module": "es2015",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "noImplicitThis": false,
    "pretty": true,
    "strictNullChecks": false,
    "target": "es2017",
    "allowJs": true
  },
  "include": [
    "./src/*"
  ],
  "exclude": [
    "node_modules"
  ]
}
